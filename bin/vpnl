#!/usr/bin/env bash

f(){
local endpoints endpoint
endpoints=$(find /home/ia/ProtonVPN/ProtonVPN_*_configs)
endpoint=$(echo "$endpoints" | fzf --tac +m -e) &&
sudo openvpn --config "$endpoint" --verb 6 --up /etc/openvpn/vpnfailsafe.sh --down /etc/openvpn/vpnfailsafe.sh --auth-user-pass /home/ia/opass.txt
}
f

